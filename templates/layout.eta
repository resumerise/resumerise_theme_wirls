<!DOCTYPE html>
<html class="<%= it.type %>">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, user-scalable=no, minimal-ui">
		<title><%= it.resume.basics?.name ? it.resume.basics.name : ''%></title>
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
		<style>
			<%~ it.css %>
			<%~ it.widgetCss %>
		</style>
	</head>
	<body>
		<main class="main container">
			<!-- BASICS -->
			<%~ include("basic", {
				resume: it.resume
			}) %>
			
			<div class="main-wrapper">
				<div class="left">
					<!-- PROJECT -->
					<%~ include("project", {
						resume: it.resume
					}) %>

					<!-- EDUCATION -->
					<%~ include("education", {
						resume: it.resume
					}) %>

					<!-- WORK -->	
					<%~ include("work", {
						resume: it.resume
					}) %>

					<!-- REFERENCE -->
					<%~ include("reference", {
						resume: it.resume
					}) %>
				</div>

				<div class="right">
					<!-- PUBLICATION -->
					<%~ include("publication", {
						resume: it.resume
					}) %>

					<!-- CERTIFICATION -->
					<%~ include("certification", {
						resume: it.resume
					}) %>
					
					<!-- SKILLS -->
					<%~ include("skills", {
						resume: it.resume
					}) %>
					
					<!-- VOLUNTEER -->
					<%~ include("volunteer", {
						resume: it.resume
					}) %>
					
					<!-- INTEREST -->
					<%~ include("interest", {
						resume: it.resume
					}) %>

					<!-- AWARDS -->
					<%~ include("awards", {
						resume: it.resume
					}) %>

					<!-- LANGUAGE -->
					<%~ include("language", {
						resume: it.resume
					}) %>
					
					<!-- PROFILE -->
					<%~ include("profile", {
						resume: it.resume
					}) %>
				</div>
			</div>
		</main>
	</body>
</html>
